eters":["Eles já sabem que você é humana."]},{"code":101,"indent":0,"parameters":["Celeste_Human",2,0,2]},{"code":401,"indent":0,"parameters":["Ah, entendo. Eles já sabem."]},{"code":101,"indent":0,"parameters":["Light-Child",1,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":241,"indent":0,"parameters":[{"name":"Dungeon1","volume":90,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["Celeste_Human",6,0,2]},{"code":401,"indent":0,"parameters":["Espera! Como eles sabem! E como você sabe"]},{"code":401,"indent":0,"parameters":["disso!"]},{"code":101,"indent":0,"parameters":["Light-Child",1,0,2]},{"code":401,"indent":0,"parameters":["Enquanto eu estava fora de controle, meus"]},{"code":401,"indent":0,"parameters":["poderes foram extendidos e pude ler a mente"]},{"code":401,"indent":0,"parameters":["deles, pelo menos um pouco."]},{"code":101,"indent":0,"parameters":["Celeste_Human",6,0,2]},{"code":401,"indent":0,"parameters":["Não, isso é mentira! Tem que ser mentira!"]},{"code":401,"indent":0,"parameters":["O que eu vou fazer! Eles sabem que eu não"]},{"code":401,"indent":0,"parameters":["sou um deles, que eu sou uma aberração!"]},{"code":401,"indent":0,"parameters":["Como eu vou encarar meu povo agora!"]},{"code":101,"indent":0,"parameters":["Light-Child",1,0,2]},{"code":401,"indent":0,"parameters":[""]},{"code":242,"indent":0,"parameters":[3]},{"code":101,"indent":0,"parameters":["Light-Child",1,0,2]},{"code":401,"indent":0,"parameters":["Você não parece ser mestiça. Como adquiriu"]},{"code":401,"indent":0,"parameters":["a forma Draco?"]},{"code":101,"indent":0,"parameters":["Celeste_Human",5,0,2]},{"code":401,"indent":0,"parameters":["É uma longa história. Se não importar de"]},{"code":401,"indent":0,"parameters":["ouvir, preciso colocar isso pra fora."]},{"code":101,"indent":0,"parameters":["Light-Child",0,0,2]},{"code":401,"indent":0,"parameters":["Eu estou aqui pra isso."]},{"code":241,"indent":0,"parameters":[{"name":"03_Childhood_Memories","vol