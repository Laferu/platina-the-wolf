{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["O experimento falhou. O receptáculo rejeitou o poder da"]},{"code":401,"indent":0,"parameters":["luz e das trevas, a Humana fugiu com a criança e o"]},{"code":401,"indent":0,"parameters":["nosso rei resolveu provocar o Guardião da Luz, fazendo"]},{"code":401,"indent":0,"parameters":["com que ele acidentalmente toque no Cristal da Alma."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Agora o Guardião da Luz enlouqueceu e quer matar a"]},{"code":401,"indent":0,"parameters":["todos nós. Percebi que ele adquiriu a mesma fraqueza"]},{"code":401,"indent":0,"parameters":["que a gente, está evitando locais com a iluminação do"]},{"code":401,"indent":0,"parameters":["sol. Irei usar isso a meu favor, mas acredito que não"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["vamos conseguir sobreviver por muito tempo."]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":8,"y":5},
{"id":3,"name":"EV003","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Livro sobre os Darkness(escrito com algum idioma da"]},{"code":401,"indent":0,"parameters":["Terra)."]},{"code":401,"indent":0,"parameters":[""]},{"code":401,"indent":0,"parameters":["\"Basicamente eles são sombras que ganharam corpos, como"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["se a natureza tentasse corrigir eles, já que este tipo"]},{"code":401,"indent":0,"parameters":["de existência deveria ser impossível. Eles não são"]},{"code":401,"indent":0,"parameters":["muito diferentes de nós em aparência, será que são"]},{"code":401,"indent":0,"parameters":["algum tipo de experiência feita a partir da gente? De"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["todas as criaturas inteligentes neste planeta, eles"]},{"code":401,"indent":0,"parameters":["são o que mais tem semelhança geneticamente falando."]},{"code":401,"indent":0,"parameters":["Talvez tenham feito a separação da sombra, que deveria"]},{"code":401,"indent":0,"parameters":["ser impossível e depois ganhou forma sólida de acordo"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["com as características comuns das criaturas deste"]},{"code":401,"indent":0,"parameters":["planeta. Mas se isso for verdade, o que será que"]},{"code":401,"indent":0,"parameters":["aconteceu com o corpo sem a sombra? Os nativos dizem"]},{"code":401,"indent":0,"parameters":["que um deus criou os Darkness, e que este deus é a"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["sombra de outro ser que expulsou todas as suas"]},{"code":401,"indent":0,"parameters":["impurezas para se tornar perfeito, assim criando o deus"]},{"code":401,"indent":0,"parameters":["bom e o deus mal. Talvez devemos acreditar nisso pois"]},{"code":401,"indent":0,"parameters":["muitas coisas neste planeta não respeitam as leis do"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["universo. Darkness não possuem reflexo, são muito"]},{"code":401,"indent":0,"parameters":["rápidos, resistentes e fortes. Possuem audição, olfato"]},{"code":401,"indent":0,"parameters":["aguçados. Visão noturna e em câmera lenta. São"]},{"code":401,"indent":0,"parameters":["completamente carnívoros\"."]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["Foram apenas alguns trechos que deu pra entender do"]},{"code":401,"indent":0,"parameters":["livro. Todo o resto está manchado de sangue."]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":12,"y":8},
